{
  "name": "@typhonjs-build-test/esm-d-ts",
  "description": "Generates a bundled Typescript declaration file from ESM source.",
  "version": "0.0.16",
  "author": "Michael Leahy <support@typhonjs.io> (https://github.com/typhonrt)",
  "type": "module",
  "main": "src/generator/index.js",
  "module": "src/generator/index.js",
  "exports": {
    ".": {
      "types": "./src/generator/index.d.ts",
      "import": "./src/generator/index.js"
    },
    "./transformer": {
      "types": "./src/transformer/index.d.ts",
      "import": "./src/transformer/index.js"
    },
    "./package.json": "./package.json"
  },
  "contributors": [
    "Michael Leahy <support@typhonjs.io> (https://github.com/typhonrt)"
  ],
  "dependencies": {
    "@rollup/plugin-alias": "^4.0.3",
    "@typhonjs-utils/file-util": "^0.0.6",
    "@typhonjs-utils/object": "^0.0.10",
    "@typhonjs-utils/package-json": "^0.6.0",
    "comment-parser": "^1.3.0",
    "es-module-lexer": "^1.2.0",
    "fs-extra": "^11.1.1",
    "resolve.exports": "^2.0.2",
    "rollup-plugin-dts": "^5.3.0",
    "typescript": ">=5.0.4",
    "upath": "^2.0.1"
  },
  "devDependencies": {
    "eslint": "^8",
    "@typhonjs-config/eslint-config": "^0.3.0"
  },
  "peerDependencies": {
    "rollup": "^3"
  },
  "engines": {
    "node": ">=14.18"
  },
  "files": [
    "/npm-shrinkwrap.json",
    "/src",
    "/types"
  ],
  "homepage": "https://github.com/typhonjs-node-build-test/esm-d-ts#readme",
  "keywords": [
    "typhonjs",
    "build",
    "esm",
    "d.ts",
    "dts",
    "declarations",
    "typescript"
  ],
  "license": "MPL-2.0",
  "repository": "github:typhonjs-node-build-test/esm-d-ts",
  "bugs": "https://github.com/typhonjs-node-build-test/esm-d-ts/issues",
  "scripts": {
    "eslint": "eslint .",
    "prepublishOnly": "npm run types",
    "types": "node -e \"import('./src/generator/index.js').then(async module => { await module.generateDTS({ input: './src/generator/index.js', output: './src/generator/index.d.ts' }); await module.generateDTS({ input: './src/transformer/index.js', output: './src/transformer/index.d.ts' }); });\""
  }
}
